

namespace Raiding
{
    public abstract class BaseHero
    {
        private string name;
        public string Name { get { return name; } set { name = value; } }
        private int power;
        public int Power { get { return power; } set { power = value; } }
        public abstract string CastAbility();
        public BaseHero(string name)
        {
            Name = name;
        }
    }
}
